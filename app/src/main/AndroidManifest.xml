<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jack.recyclerviewstudy">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RecyclerViewStudy">
        <activity android:name=".SimpleUseWithItemLineActivity" />
        <activity android:name=".SimpleUseWithItemGroupActivity" />
        <activity android:name=".SimpleUseWithItemGroup2Activity" />
        <activity android:name=".SimpleUseWithItemTagActivity" />
        <activity android:name=".SimpleUseWithViewTypeActivity" />
        <activity android:name=".SimpleUseWithBaseAdapterActivity" />
        <activity android:name=".SimpleUseGridLayoutActivity" />
        <activity android:name=".SimpleUseStaggeredGridLayoutActivity" />
        <activity android:name=".ListViewActivity" />
        <activity android:name=".SimpleUseActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>